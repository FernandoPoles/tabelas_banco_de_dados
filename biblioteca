CREATE TABLE Autor (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    nome TEXT,
    email TEXT,
    nacionalidade TEXT,
    nasc TEXT
);

CREATE TABLE Livro (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    titulo TEXT,
    paginas INTEGER,
    acabamento TEXT,
    editora TEXT,
    id_autor INTEGER,
    FOREIGN KEY (id_autor) REFERENCES Autor(id)
);

INSERT INTO Autor (nome, email, nacionalidade, nasc) VALUES 
  ('Machado de Assis', 'machado@example.com', 'Brasileiro', '1839-06-21'),
  ('George Orwell', 'george@example.com', 'Brit√¢nico', '1903-06-25');

