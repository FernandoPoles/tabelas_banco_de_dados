CREATE TABLE Produto (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    nome TEXT,
    preco REAL,
    qtd_estoque INTEGER,
    nome_marca TEXT,
    sac_marca TEXT,
    nacionalidade_marca TEXT
);

